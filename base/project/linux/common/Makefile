include ../config.mk

SRC_DIR_DEBUG = ../../../common
OUT_DEBUG = ./obj/Debug/libcommon.a

SRC_DIR_RELEASE = ../../../common
OUT_RELEASE = ./obj/Release/libcommon.a

OBJ_DEBUG =  $(OBJDIR_DEBUG)/common/sa_filetext.o $(OBJDIR_DEBUG)/common/sa_utility.o

OBJ_RELEASE =  $(OBJDIR_RELEASE)/common/sa_filetext.o $(OBJDIR_RELEASE)/common/sa_utility.o

all: debug release

clean: clean_debug clean_release

before_debug: 
	test -d $(OBJDIR_DEBUG)/common/linux || mkdir -p $(OBJDIR_DEBUG)/common/linux
	test -d $(OBJDIR_DEBUG)/common/sys	|| mkdir -p $(OBJDIR_DEBUG)/common/sys
	test -d $(LIB_TARGET_DIR)/debug || mkdir -p $(LIB_TARGET_DIR)/debug

after_debug: 
	cp ./obj/Debug/libcommon.a $(LIB_TARGET_DIR)/debug/libcommon.a

debug: before_debug out_debug after_debug

out_debug: before_debug $(OBJ_DEBUG) $(DEP_DEBUG)
	$(AR) rcs $(OUT_DEBUG) $(OBJ_DEBUG)

$(OBJDIR_DEBUG)/common/sa_filetext.o: $(SRC_DIR_DEBUG)/sa_filetext.cpp
	$(CXX) $(CFLAGS_DEBUG) $(INC_DEBUG) -c $(SRC_DIR_DEBUG)/sa_filetext.cpp -o $(OBJDIR_DEBUG)/common/sa_filetext.o

$(OBJDIR_DEBUG)/common/sa_utility.o: $(SRC_DIR_DEBUG)/sa_utility.cpp
	$(CXX) $(CFLAGS_DEBUG) $(INC_DEBUG) -c $(SRC_DIR_DEBUG)/sa_utility.cpp -o $(OBJDIR_DEBUG)/common/sa_utility.o

clean_debug: 
	rm -f $(OBJ_DEBUG) $(OUT_DEBUG)
	rm -rf $(OBJDIR_DEBUG)/common/linux

before_release: 
	test -d $(OBJDIR_RELEASE)/common/linux || mkdir -p $(OBJDIR_RELEASE)/common/linux
	test -d $(LIB_TARGET_DIR)/release || mkdir -p $(LIB_TARGET_DIR)/release
after_release: 
	cp ./obj/Release/libcommon.a $(LIB_TARGET_DIR)/release/libcommon.a

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(AR) rcs $(OUT_RELEASE) $(OBJ_RELEASE)

$(OBJDIR_RELEASE)/common/sa_filetext.o: $(SRC_DIR_RELEASE)/sa_filetext.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c $(SRC_DIR_RELEASE)/sa_filetext.cpp -o $(OBJDIR_RELEASE)/common/sa_filetext.o

$(OBJDIR_RELEASE)/common/sa_utility.o: $(SRC_DIR_RELEASE)/sa_utility.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c $(SRC_DIR_RELEASE)/sa_utility.cpp -o $(OBJDIR_RELEASE)/common/sa_utility.o

clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf $(OBJDIR_RELEASE)/common/linux

.PHONY: before_debug after_debug clean_debug before_release after_release clean_release

